{
  "name": "@snaveevans/refactored-fishstick",
  "version": "1.1.1",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*/index.d.ts",
      "import": "./dist/*/index.js"
    },
    "./*/*": {
      "types": "./dist/*/*.d.ts",
      "import": "./dist/*/*.js"
    },
    "./*.js": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "build:storybook": "storybook build",
    "dev": "rslib build --watch",
    "format": "prettier --write .",
    "lint": "eslint .",
    "storybook": "storybook dev",
    "test": "vitest run"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@rsbuild/core": "^1.5.17",
    "@rsbuild/plugin-react": "^1.4.1",
    "@rslib/core": "^0.16.1",
    "@storybook/addon-docs": "^9.1.13",
    "@storybook/addon-onboarding": "^9.1.13",
    "@storybook/react": "^9.1.13",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.2.2",
    "eslint": "^9.35.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "storybook": "^9.1.13",
    "storybook-addon-rslib": "^2.1.2",
    "storybook-react-rsbuild": "^2.1.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.43.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "private": false
}
